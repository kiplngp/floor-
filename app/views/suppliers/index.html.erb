<!--sign in area defined-->
<div id="signIn_area">
	<div class="sign_in_tp">
    	<div class="sign_left">
        	<div class="sign_right">
             	<h1>List of Suppliers</h1>
          </div>
        </div>
    </div>
    
	<div class="sign_left_slc">
    
    	<div class="sign_right_slc">
        <div class="userListing">
        	<table width="100%" border="0" cellspacing="0" cellpadding="0">
  <tr>
    <th align="left">S.N.</th>
    <th align="left">Account Reference</th>
    <th align="left">Company Name</th>
    <th align="left">Address Line1</th>
    
    
    <th align="left" class="border-right">Action</th>
    </tr>
  <% @suppliers.each do |supplier| %>
    <tr>
      <td class="border-bottom"><%= supplier.id %></td>
      <td class="border-bottom"><%= supplier.account_reference %></td>
      <td class="border-bottom"><%= supplier.company_name %></td>
      <td class="border-bottom"><%= supplier.address_line1 %></td>
      
      <td class="border-bottom-right"><%= link_to 'View', supplier %> | <%= link_to 'Edit', edit_supplier_path(supplier) %> | <%= link_to 'Destroy', supplier , :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

        </div>
        
         <div class="clear"></div>       
      </div>
    </div>
    
    <div class="sign_ftr_slc">
    	<div class="sign_ftr_left">
        	<div class="sign_ftr_right">
            
            	
              
                
            </div>
        </div>
    </div>
</div>
<!--sign in area defined-->
